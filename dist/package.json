{
  "name": "@thisissoon/angular-inviewport",
  "version": "5.0.0",
  "description": "A simple lightweight library for Angular that detects when an element is within the browser viewport and adds a sn-viewport--in or sn-viewport--out class to the element.",
  "keywords": [
    "angular",
    "directive",
    "viewport",
    "in viewport",
    "scroll",
    "resize",
    "ngx-inviewport",
    "ngx-in-viewport"
  ],
  "author": "SOON_ <dorks@thisissoon.com>",
  "contributors": [
    "Ed Opare-Aryee <ed@thisissoon.com>",
    "Jack matthews <jack@thisissoon.com>",
    "Sergey Krilov <serg.kr@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mirazasx/angular-inviewport-ng13"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0"
  },
  "private": false,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "tslint --fix",
      "git add"
    ],
    "src/**/*.{css,scss}": [
      "stylelint --syntax scss --fix",
      "git add"
    ],
    "src/**/*.{ts,js,json,css,scss}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "module": "fesm2015/thisissoon-angular-inviewport.mjs",
  "es2020": "fesm2020/thisissoon-angular-inviewport.mjs",
  "esm2020": "esm2020/thisissoon-angular-inviewport.mjs",
  "fesm2020": "fesm2020/thisissoon-angular-inviewport.mjs",
  "fesm2015": "fesm2015/thisissoon-angular-inviewport.mjs",
  "typings": "thisissoon-angular-inviewport.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./thisissoon-angular-inviewport.d.ts",
      "esm2020": "./esm2020/thisissoon-angular-inviewport.mjs",
      "es2020": "./fesm2020/thisissoon-angular-inviewport.mjs",
      "es2015": "./fesm2015/thisissoon-angular-inviewport.mjs",
      "node": "./fesm2015/thisissoon-angular-inviewport.mjs",
      "default": "./fesm2020/thisissoon-angular-inviewport.mjs"
    },
    "./src": {
      "types": "./src/thisissoon-angular-inviewport-src.d.ts",
      "esm2020": "./esm2020/src/thisissoon-angular-inviewport-src.mjs",
      "es2020": "./fesm2020/thisissoon-angular-inviewport-src.mjs",
      "es2015": "./fesm2015/thisissoon-angular-inviewport-src.mjs",
      "node": "./fesm2015/thisissoon-angular-inviewport-src.mjs",
      "default": "./fesm2020/thisissoon-angular-inviewport-src.mjs"
    }
  },
  "sideEffects": false
}